#!/bin/bash

printf "%-35s%-8s%-20s\n" "$(date +%a)" "%a" "locale's abbreviated weekday name (e.g., Sun)"
printf "%-35s%-8s%-20s\n" "$(date +%A)" "%A" "locale's full weekday name (e.g., Sunday)"
printf "%-35s%-8s%-20s\n" "$(date +%b)" "%b" "locale's abbreviated month name (e.g., Jan)"
printf "%-35s%-8s%-20s\n" "$(date +%B)" "%B" "locale's full month name (e.g., January)"
printf "%-35s%-8s%-20s\n" "$(date +%c)" "%c" "locale's date and time (e.g., Thu Mar  3 23:05:25 2005)"
printf "%-35s%-8s%-20s\n" "$(date +%C)" "%C" "century; like %Y, except omit last two digits (e.g., 20)"
printf "%-35s%-8s%-20s\n" "$(date +%d)" "%d" "day of month (e.g, 01)"
printf "%-35s%-8s%-20s\n" "$(date +%D)" "%D" "date; same as %m/%d/%y"
printf "%-35s%-8s%-20s\n" "$(date +%e)" "%e" "day of month, space padded; same as %_d"
printf "%-35s%-8s%-20s\n" "$(date +%F)" "%F" "full date; same as %Y-%m-%d"
printf "%-35s%-8s%-20s\n" "$(date +%g)" "%g" "last two digits of year of ISO week number (see %G)"
printf "%-35s%-8s%-20s\n" "$(date +%G)" "%G" "year of ISO week number (see %V); normally useful only with %V"
printf "%-35s%-8s%-20s\n" "$(date +%h)" "%h" "same as %b"
printf "%-35s%-8s%-20s\n" "$(date +%H)" "%H" "hour (00..23)"
printf "%-35s%-8s%-20s\n" "$(date +%I)" "%I" "hour (01..12)"
printf "%-35s%-8s%-20s\n" "$(date +%j)" "%j" "day of year (001..366)"
printf "%-35s%-8s%-20s\n" "$(date +%k)" "%k" "hour ( 0..23)"
printf "%-35s%-8s%-20s\n" "$(date +%l)" "%l" "hour ( 1..12)"
printf "%-35s%-8s%-20s\n" "$(date +%m)" "%m" "month (01..12)"
printf "%-35s%-8s%-20s\n" "$(date +%M)" "%M" "minute (00..59)"
printf "%-35s%-8s%-20s\n" "$(date +%n)" "%n" "a newline"
printf "%-35s%-8s%-20s\n" "$(date +%N)" "%N" "nanoseconds (000000000..999999999)"
printf "%-35s%-8s%-20s\n" "$(date +%p)" "%p" "locale's equivalent of either AM or PM; blank if not known"
printf "%-35s%-8s%-20s\n" "$(date +%P)" "%P" "like %p, but lower case"
printf "%-35s%-8s%-20s\n" "$(date +%r)" "%r" "locale's 12-hour clock time (e.g., 11:11:04 PM)"
printf "%-35s%-8s%-20s\n" "$(date +%R)" "%R" "24-hour hour and minute; same as %H:%M"
printf "%-35s%-8s%-20s\n" "$(date +%s)" "%s" "seconds since 1970-01-01 00:00:00 UTC"
printf "%-35s%-8s%-20s\n" "$(date +%S)" "%S" "second (00..60)"
printf "%-35s%-8s%-20s\n" "\t" "%t" "a tab"
printf "%-35s%-8s%-20s\n" "$(date +%T)" "%T" "time; same as %H:%M:%S"
printf "%-35s%-8s%-20s\n" "$(date +%u)" "%u" "day of week (1..7); 1 is Monday"
printf "%-35s%-8s%-20s\n" "$(date +%U)" "%U" "week number of year, with Sunday as first day of week (00..53)"
printf "%-35s%-8s%-20s\n" "$(date +%V)" "%V" "ISO week number, with Monday as first day of week (01..53)"
printf "%-35s%-8s%-20s\n" "$(date +%w)" "%w" "day of week (0..6); 0 is Sunday"
printf "%-35s%-8s%-20s\n" "$(date +%W)" "%W" "week number of year, with Monday as first day of week (00..53)"
printf "%-35s%-8s%-20s\n" "$(date +%x)" "%x" "locale's date representation (e.g., 12/31/99)"
printf "%-35s%-8s%-20s\n" "$(date +%X)" "%X" "locale's time representation (e.g., 23:13:48)"
printf "%-35s%-8s%-20s\n" "$(date +%y)" "%y" "last two digits of year (00..99)"
printf "%-35s%-8s%-20s\n" "$(date +%Y)" "%Y" "year"
printf "%-35s%-8s%-20s\n" "$(date +%z)" "%z" "+hhmm numeric timezone (e.g., -0400)"
printf "%-35s%-8s%-20s\n" "$(date +%:z)" "%:z" "+hh:mm numeric timezone (e.g., -04:00)"
printf "%-35s%-8s%-20s\n" "$(date +%::z)" "%::z" "+hh:mm:ss numeric time zone (e.g., -04:00:00)"
printf "%-35s%-8s%-20s\n" "$(date +%:::z)" "%:::z" "numeric time zone with : to necessary precision (e.g., -04, +05:30)"
printf "%-35s%-8s%-20s\n" "$(date +%Z)" "%Z" "alphabetic time zone abbreviation (e.g., EDT)"
